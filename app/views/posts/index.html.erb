<div class="columns is-centered">
  <div class="column is-one-third">
    <div class="section">
      <%= render 'groups' %>
    </div>
  </div>
  <div class="column is-one-third" id="posts">
    <div class="section">
      <%= render '/shared/post' %>
      <p class='mb-6'></p>
      <%= render '/shared/feed', class: 'feed' %>
    </div>
  </div>
  <div class="column is-one-third">
    <div class="section">
      <%= render 'suggestions' %>
    </div>
  </div>
</div>